   <children>
      <MediaView fx:id="vdo" fitHeight="350.0" fitWidth="550.0" layoutX="25.0" layoutY="14.0" />
      <Button fx:id="backToFirstPage" layoutX="534.0" layoutY="364.0" mnemonicParsing="false" onAction="#backToFirstPage" text="Back" />
      <Button fx:id="playButton" layoutX="25.0" layoutY="364.0" mnemonicParsing="false" onAction="#clickPlay" text="play" />
      <Button fx:id="pauseButton" layoutX="100.0" layoutY="364.0" mnemonicParsing="false" onAction="#clickStop" text="pause" />
   </children>
</AnchorPane>
